{
  "name": "sample-sheet",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://billyct.github.io/sample-sheet",
  "dependencies": {
    "antd": "^3.4.0",
    "lodash": "^4.17.5",
    "number-precision": "^1.2.0",
    "react": "^16.3.1",
    "react-datasheet": "^1.3.3",
    "react-dom": "^16.3.1",
    "react-redux": "^5.0.7",
    "react-scripts": "1.1.4",
    "redux": "^3.7.2",
    "styled-jsx": "^2.2.6",
    "xlsx": "^0.12.8"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-app-rewired eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "jscsConfig": {
    "preset": "airbnb",
    "requireCurlyBraces": null,
    "disallowQuotedKeysInObjects": false,
    "requirePaddingNewLinesAfterBlocks": {
      "allExcept": [
        "inCallExpressions",
        "inNewExpressions",
        "inArrayExpressions",
        "inProperties",
        "singleLine"
      ]
    }
  },
  "devDependencies": {
    "babel-plugin-import": "^1.7.0",
    "gh-pages": "^1.1.0",
    "react-app-rewired": "^1.5.0",
    "redux-devtools-extension": "^2.13.2"
  }
}
